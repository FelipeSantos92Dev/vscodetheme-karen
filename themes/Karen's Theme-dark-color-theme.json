        "meta.decorator variable.other.readwrite.ts",
        "variable.parameter.gdscript",
        "entity.name.variable.parameter",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d28ad4"
      }
    },
    {
      "scope": ["keyword.control.export", "support.type.object.module"],
